generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model UserModel {
  id       String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  email    String @unique
  password String
}

model TodoModel {
  id       String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  title    String
  priority String
  workflow String
  date     String
  author   String
}
